Language "Golang"

[
  #1 Lexical
//  #2 Types
  #3 Expressions
  #4 Statements
  #5 Declarations
  #A Disambiguation
]

Syntax
  START : start ::= statement

Semantics
  start[[ START:start ]] : =>null-type

Rule
  start[[Statement]]
    = initialise-binding
      initialise-storing
      finalise-failing
        execute[[Statement]]
